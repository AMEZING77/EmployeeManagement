@{
}

@model IEnumerable< EmployeeManagement.Models.Employee>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Department</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Department</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>
